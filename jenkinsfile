pipeline{
	tools{
		jdk 'java'
		maven 'maven'
		}
		agent none(any)
		stages{
			stage('checkout'){
				agent any
				steps{
					echo 'CLONING....'
					git 'https://github.com/sree9048/gitdemo.git'
				}
			}	
			stage('compile'){
				agent any
				steps{
					echo 'COMPILING...'
					sh 'mvn compile'
				}
			}
			stage('codeReview'){
				agent any
				steps{
					echo 'CODEREVIEW'
					sh 'mvn pmd:pmd'
				}
			}
			stage('unitTest'){
				agent any
				steps{
					sh 'mvn test'
				}
				
			}
			stage('package'){
				agent any
				steps{
					sh 'mvn test'
				}
			}
			
		}
}
